{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": { 
        "linebreak-style" : "off",
        "indent" : "off",
        "eol-last" : "off",
        "no-underscore-dangle" : "off",
        "no-useless-constructor" :"off"
    },
    "plugin" : ["jasmine", "codeceptjs"]
}
